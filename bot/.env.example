# Panel URL
PANEL_URL=https://yemek-metasal-panel.xgpvni.easypanel.host

# Botun dinlediği port
BOT_PORT=3001

# Vision API Ayarları
VISION_API_TOKEN=Fuatumulu2%%%
VISION_LOCAL_API=http://127.0.0.1:3030
VISION_FOLDER_ID=your_folder_id_here

# Görev kontrol aralığı (milisaniye)
TASK_CHECK_INTERVAL=10000

# İşlem arası bekleme süresi (milisaniye)
ACTION_DELAY=3000

# Thread Ayarları
THREAD_COUNT=2          # Aynı anda çalışacak maksimum profil sayısı
THREAD_DELAY=35000      # Thread'ler arası bekleme süresi (milisaniye)

# Mobil Proxy Change URL Tanımları
# Format: PROXY_CHANGE_<TANIMLAYICI>=<host>:<port>|<change_url>
# Her carrier için bir tanım ekleyin:
PROXY_CHANGE_1=188.245.86.209:8080|http://188.245.86.209:8080/apix/reset_ip_secure?hash=8f605eb74a2f
# İkinci carrier örneği:
# PROXY_CHANGE_2=1.2.3.4:8080|http://1.2.3.4:8080/apix/reset_ip_secure?hash=XXXXXX

# Proxy Change Ayarları
PROXY_CHANGE_TIMEOUT=60000     # Change isteği timeout (ms)
PROXY_CHANGE_COOLDOWN=30000    # Change sonrası bekleme (ms)

# Gönderi Erişim Takibi
POST_ACCESS_CHECK_INTERVAL=1800000  # 30 dakika (ms)

